{
  "name": "agritoca-api",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "env-cmd -f ./config/test.env jest --watch --runInBand --verbose --silent",
    "start": "node index.js",
    "dev": "env-cmd -f ./config/dev.env nodemon index.js",
    "migrate": "knex migrate:latest",
    "rollback": "knex migrate:rollback",
    "seed": "knex seed:run",
    "dev:migrate": "env-cmd -f ./config/dev.env knex migrate:latest",
    "dev:rollback": "env-cmd -f ./config/dev.env knex migrate:rollback",
    "dev:clean": "env-cmd -f ./config/dev.env knex --knexfile resetterfile.js seed:run",
    "dev:seed": "env-cmd -f ./config/dev.env knex seed:run",
    "dev:remake": "env-cmd -f ./config/dev.env knex --knexfile resetterfile.js seed:run; env-cmd -f ./config/dev.env knex migrate:latest; env-cmd -f ./config/dev.env knex seed:run"
  },
  "keywords": [],
  "author": "Edmundo Paulino",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.20.0",
    "celebrate": "^12.2.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "hashids": "^2.2.1",
    "jsonwebtoken": "^8.5.1",
    "knex": "^0.21.3",
    "multer": "^1.4.2",
    "pg": "^8.3.0",
    "sharp": "^0.26.0"
  },
  "devDependencies": {
    "env-cmd": "^10.1.0",
    "jest": "^26.2.2",
    "nodemon": "^2.0.4",
    "supertest": "^4.0.2"
  }
}
